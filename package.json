{
  "name": "schedule-arranger",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "~12"
  },
  "scripts": {
    "format": "prettier --ignore-path ./.prettierignore --write ./**/*.{j,t}s{,x}",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "tsc": "tsc -p ./tsconfig.json --noEmit",
    "build:client": "webpack",
    "build:client:watch": "webpack watch",
    "build:server": "ts-node build.ts",
    "build": "webpack && ts-node build.ts",
    "dev": "webpack & ts-node -r tsconfig-paths/register --files src/index.ts",
    "dev:watch": "webpack watch & ts-node-dev -r tsconfig-paths/register --respawn --files src/index.ts",
    "start": "yarn build && node -r module-alias/register ./dist/index.js --env production",
    "test": "jest"
  },
  "_moduleAliases": {
    "~": "dist"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.1",
    "helmet": "^4.3.1",
    "http-errors": "^1.8.0",
    "http-status-codes": "^2.1.4",
    "jquery": "^3.5.1",
    "jsonfile": "^6.1.0",
    "log4js": "^6.3.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-github2": "^0.1.12",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "popper.js": "^1.16.1",
    "pug": "^3.0.0",
    "sequelize": "^6.3.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@types/bootstrap": "^5.0.5",
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.11.0",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.6",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.20",
    "@types/jquery": "^3.5.5",
    "@types/jsonfile": "^6.0.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.20",
    "@types/passport": "^1.0.5",
    "@types/passport-github2": "^1.2.4",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.4",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-webpack-plugin": "^2.4.3",
    "find": "^0.3.0",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "fs-extra": "^9.0.1",
    "jasmine": "^3.6.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "passport-stub": "^1.1.1",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.4.0"
  }
}
